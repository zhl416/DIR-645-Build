### Target Vendor/Product
CONFIG_VENDOR=DLINK
CONFIG_PRODUCT=RT3662

### Target ProductID
CONFIG_FIRMWARE_PRODUCT_ID="DIR-645-A1"

### Linux kernel and toolchain
CONFIG_LINUXDIR=linux-3.4.x

### CPU配置
CONFIG_FIRMWARE_CPU_500MHZ=y

### RAM配置
CONFIG_RALINK_RAM_SIZE=64

### Flash配置
CONFIG_MTD_STORE_PART_SIZ=65536
CONFIG_MTD_FIRMWARE_PART_SIZ=7012352

### 交换机配置（RTL8367RB）
CONFIG_RTL8367_API_8367B=y
CONFIG_RAETH_GMAC2=y
CONFIG_RAETH_RGMII_MODE=y
CONFIG_RTL8367_MCM_WAN_PORT=4
CONFIG_RTL8367_MCM_LAN_WAN_PARTITION=y

### 无线配置
CONFIG_RT_FIRST_CARD=3662
CONFIG_RT_FIRST_IF_RF_OFFSET=0x40000
CONFIG_RT_FIRST_IF_EEPROM="flash"
CONFIG_RT_FIRST_IF_RF_TYPE=3
CONFIG_RT_FIRST_IF_HAS_2T2R=y
CONFIG_RT_FIRST_IF_MAX_TX_COUNT=2
CONFIG_RT_FIRST_IF_MAX_RX_COUNT=2
CONFIG_RT_FIRST_IF_RANGE_2GHZ=y
CONFIG_RT_FIRST_IF_RANGE_5GHZ=n

### USB支持
CONFIG_FIRMWARE_ENABLE_USB=y
CONFIG_FIRMWARE_ENABLE_FAT=y
CONFIG_FIRMWARE_ENABLE_EXFAT=y
CONFIG_FIRMWARE_ENABLE_EXT2=y
CONFIG_FIRMWARE_ENABLE_EXT3=y
CONFIG_FIRMWARE_ENABLE_EXT4=y

### 基础功能
CONFIG_FIRMWARE_ENABLE_IPV6=y
CONFIG_FIRMWARE_INCLUDE_DROPBEAR=y

### 其他功能（按需启用）
CONFIG_FIRMWARE_INCLUDE_DDNS_SSL=y
CONFIG_FIRMWARE_INCLUDE_HTTPS=y
CONFIG_FIRMWARE_INCLUDE_SFTP=y
CONFIG_FIRMWARE_INCLUDE_SAMBA=y
CONFIG_FIRMWARE_INCLUDE_FTPD=y
